{{# buttonDropdown }}
<div class="btn-group">
	<button type="button" class="btn {{ buttonClass }} dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">{{ cta }} <span class="caret"></span></button>
	<ul class="dropdown-menu">
		{{# dropdownList }}
    	<li><a href="#">{{ listText }}</a></li>
		{{/ dropdownList }}
	</ul>
</div><!-- /btn-group -->
{{/ buttonDropdown }}